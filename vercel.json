{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/stockfish/stockfish.js",
        "dest": "/public/stockfish/stockfish.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }